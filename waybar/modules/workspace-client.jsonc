{
  "custom/workspaces": {
    "exec": "~/.config/waybar/scripts/clients.sh",
    "format": "{}",
    "interval": 2,
    "max-length": 8,
    "min-length": 8,
    "on-click": "~/.config/waybar/scripts/clients.sh client",
    "on-click-right": "~/.config/waybar/scripts/clients.sh workspace",
    "return-type": "json",
    "tooltip": true,
  },
}
